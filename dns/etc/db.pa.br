;
; BIND pa.br
;
$TTL	604800
@	IN	SOA	pa.br. root.pa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN  MX 10 mail.pa.br.
@	IN	NS	ns1.pa.br.

@	IN	NS	ns2.pa.br.
@	IN	A	127.0.0.1

ns1			A		172.29.240.1
mail		A 		10.24.6.114
ns2 		CNAME   127.0.0.1
www			CNAME	ns1
