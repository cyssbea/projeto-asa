;
; BIND pa.br
;
$TTL	604800
@	IN	SOA	pa.br. root.pa.br. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN  MX 10 mail.pa.br.
@	IN	NS	ns1.pa.br.
@	IN	A	192.168.0.20

ns1			A		192.168.0.20
intranet		A		192.168.0.20
webmail			A		192.168.0.20
portal			A		192.168.0.20
www			CNAME		ns1
